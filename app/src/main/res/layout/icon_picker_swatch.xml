<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView android:id="@+id/icon_picker_swatch_icon"
            android:layout_width="@dimen/xlarge_icon"
            android:layout_height="@dimen/xlarge_icon"
            android:layout_marginRight="@dimen/margin_4"
            android:layout_marginTop="@dimen/margin_4"
            android:src="@drawable/abc_vector_test"/>
        <TextView
            android:id="@+id/icon_picker_swatch_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/small_text"
            android:text="category"
            android:layout_marginTop="@dimen/margin_2"
            android:layout_gravity="center"
            android:textColor="@color/gray"/>
        <ImageView
            android:id="@+id/icon_picker_swatch_ticker"
            android:layout_width="@dimen/normal_icon"
            android:layout_height="@dimen/normal_icon"
            android:src="@mipmap/question_mark"
            android:layout_marginTop="@dimen/margin_5h"
            android:layout_marginLeft="@dimen/margin_16"
            />
    </LinearLayout>

</merge>